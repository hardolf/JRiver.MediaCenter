<?xml version="1.0" encoding="utf-8"?>
<!--
CajunLyrics Lyrics API
https://www.cajunlyrics.com/?page=api
No user ID or token required

See also: https://www.programmableweb.com/api/cajunlyrics-rest-api
-->
<configuration>

    <appSettings>
        <add key="Company" value="CajunLyrics" />
        <add key="CreditTextFormat" value="

-----------------------------------------------------------
Found by LyricsFinder for JRiver MediaCenter on {Date}
Lyrics found in {ServiceName}
{Company} - {CreditUrl}
{ServiceName} - {ServiceUrl}
-----------------------------------------------------------
"/>
        <add key="CreditUrl" value="https://www.cajunlyrics.com" />
        <add key="DateFormat" value="yyyy.MM.dd" />
        <add key="ServiceName" value="CajunLyrics" />
        <add key="ServiceUrl" value="http://api.cajunlyrics.com" />
        <add key="Comment" value="No user ID or token required." />
    </appSettings>

    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>

</configuration>