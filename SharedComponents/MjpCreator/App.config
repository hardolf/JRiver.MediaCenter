            <setting name="LogHeader1" serializeAs="String">
                <value>====================================================================================================</value>
            </setting>
            <setting name="LogHeader2" serializeAs="String">
                <value>----------------------------------------------------------------------------------------------------</value>
            </setting>
        </MediaCenter.SharedComponents.Properties.Settings>
    </applicationSettings>

</configuration>
